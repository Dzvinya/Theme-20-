public class Task4 {
    public static void main(String[] args) {
        // Даны три вещественных числа a,b,c. Проверить:
        // 1.выполняется ли неравенство a<b<c;
        // 2.выполняется ли не равенство b>a>c.

        System.out.println("Введите три вещественных числа:");
        Scanner sc = new Scanner(System.in);
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        double c = sc.nextDouble();
        sc.close();

        if ((a<b)&&(b<c)) {
            System.out.println("1.Неравенство выполняется");
        } else {
            System.out.println("1.Неравенство не выполняется");
        }
        if ((b>a)&&(a>c)) {
            System.out.println("2.Неравенство выполняется");
        } else {
            System.out.println("2.Неравенство не выполняется");
        }
        }
}
